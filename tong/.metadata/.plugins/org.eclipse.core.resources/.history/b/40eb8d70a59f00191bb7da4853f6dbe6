package unsw.enrolment;

public class Individual_mark implements Mark{

	private String name;
	private int mark;
	
	public Individual_mark(String name, int mark) {
		this.name = name;
		this.mark = mark;
	}
	
	
	@Override
	public boolean isPassing() {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public int getTotalMark() {
		// TODO Auto-generated method stub
		return mark;
	}

}
