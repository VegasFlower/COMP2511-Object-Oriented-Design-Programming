package unsw.venues;

import java.time.LocalDate;
import java.util.ArrayList;

public class Room {
	private String name;
	private String size;
	private Venue venue;
	private ArrayList<LocalDate> dates = new ArrayList<LocalDate>();
	
	public Room(String name, String size, Venue venue) {
		this.name = name;
		this.size = size;
		this.venue = venue;
	}

	public String getName() {
		return name;
	}

	public Venue getVenue() {
		return venue;
	}
	public String getSize() {
		return size;
	}
}
