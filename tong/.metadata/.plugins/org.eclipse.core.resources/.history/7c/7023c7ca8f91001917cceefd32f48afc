package unsw.venues;

import java.util.ArrayList;

public class Venue {
	private String name;
	private ArrayList<Room> rooms_venue;
	
	public Venue(String name) {
		this.setName(name);
		rooms_venue = new ArrayList<>();
	}

	public void addRoom(Room room) {
//		Room new_room = new Room(room, size);
		rooms_venue.add(room);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
